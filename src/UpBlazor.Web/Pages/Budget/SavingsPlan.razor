@page "/budget/savings-plan"

@using UpBlazor.Web.Shared.Pages.Budget.SavingsPlan

<Title Level="3">Savings Plan</Title>

<UserIdWrapper Context="userId">
    <UpApiWrapper Context="accounts" Action="api => api.GetAccountsAsync()" 
                  T="PaginatedDataResponse<AccountResource>">
        <RepositoryWrapper TRepository="IIncomeRepository"
                           Action="x => x.GetAllByUserIdAsync(userId)"
                           TResponse="IReadOnlyList<Core.Models.Income>">
            <SavingsPlanInner/>
        </RepositoryWrapper>
    </UpApiWrapper>
</UserIdWrapper>


