@page "/budget/planner"

@using UpBlazor.Web.Shared.Pages.Budget.Planner

<UserIdWrapper Context="userId">
    <UpApiWrapper Context="accounts"
                  Action="api => api.GetAccountsAsync()"
                  T="PaginatedDataResponse<AccountResource>">
        <RepositoryWrapper TRepository="IIncomeRepository"
                           Action="x => x.GetAllByUserIdAsync(userId)"
                           TResponse="IReadOnlyList<Core.Models.Income>">
            <Title Level="3">Income Planner</Title>
            <PlannerInner/>
        </RepositoryWrapper>
    </UpApiWrapper>
</UserIdWrapper>