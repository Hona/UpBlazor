@layout MainLayoutWrapper

@page "/signout/success"

@inject NavigationManager _navigationManager

<Result
    Status="success"
    Title="Success"
    SubTitle="You are now signed out."
>
    <Extra>
        <Template>
            <Button OnClick="@(() => _navigationManager.NavigateTo("/signin", true))">Switch User</Button>
        </Template>
    </Extra>
</Result>